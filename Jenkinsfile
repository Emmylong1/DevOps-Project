                echo 'Build The Pipeline..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing The Pipeline..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying The Pipeline....'
            }
        }
    }
}
